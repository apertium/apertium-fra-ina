<?xml version="1.0" encoding="UTF-8"?>
<modes>

  <mode name="fr-morph">
    <pipeline>
      <program name="lt-proc -w">
        <file name="fr.automorf.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="fr-gener">
    <pipeline>
      <program name="lt-proc -g">
        <file name="fr.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="fr-tagger">
    <pipeline>
      <program name="lt-proc -w">
        <file name="fr.automorf.bin"/>
      </program>
      <program name="cg-proc -w">
        <file name="fr.rlx.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="fr-disam">
    <pipeline>
      <program name="lt-proc -w">
        <file name="fr.automorf.bin"/>
      </program>
      <program name="cg-conv -a -l" />
      <program name="vislcg3 --trace -g">
        <file name="apertium-fr.fr.rlx"/>
      </program>
    </pipeline>
  </mode>

</modes>

