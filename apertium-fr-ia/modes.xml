  <mode name="fr-ia" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="fr-ia.automorf.bin"/>
      </program>


      <program name="cg-proc -w">
        <file name="fr-ia.rlx.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="fr-ia.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="fr-ia.autobil.bin"/>
      </program>
      <program name="lrx-proc -m">
        <file name="fr-ia.autolex.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-fr-ia.fr-ia.t1x"/>
        <file name="fr-ia.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-fr-ia.fr-ia.t2x"/>
        <file name="fr-ia.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-fr-ia.fr-ia.t3x"/>
        <file name="fr-ia.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="fr-ia.autogen.bin"/>
      </program>

      <program name="lt-proc -p">
        <file name="fr-ia.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

